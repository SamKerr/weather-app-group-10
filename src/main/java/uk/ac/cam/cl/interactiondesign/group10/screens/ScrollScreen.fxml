<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox fx:id="root" prefHeight="800" prefWidth="450" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.cam.cl.interactiondesign.group10.screens.ScrollScreen">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" fx:id="textTitle" />
        <Text fx:id="textCurrentLocation" strokeType="OUTSIDE" strokeWidth="0.0" />

      <ScrollPane prefHeight="200.0" prefWidth="Infinity">
         <content>
            <VBox fx:id="scrollContents" prefHeight="200.0" prefWidth="100.0" />
         </content>
      </ScrollPane>

        <Button minWidth="-Infinity" mnemonicParsing="false" onAction="#goBack" text="Back" />
    </children>
</VBox>
